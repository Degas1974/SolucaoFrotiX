{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "MemoryManagement": {
    "EnablePeriodicGC": true,
    "GCIntervalMinutes": 5,
    "MaxMemoryMB": 2048
  }
}
