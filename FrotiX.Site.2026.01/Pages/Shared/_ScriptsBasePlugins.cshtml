<!-- Syncfusion CLDR + locale (pt-BR) -->
<script>
	(function () {
		'use strict';

		try {
			if (!(window.ej && ej.base && ej.base.loadCldr)) {
				return;
			}

			function loadJsonSync(url) {
				try {
					var ajax = new ej.base.Ajax(url, 'GET', false);
					var data = null;
					ajax.onSuccess = function (value) {
						try {
							data = JSON.parse(value);
						} catch (e) {
							data = null;
						}
					};
					ajax.onFailure = function () {
						data = null;
					};
					ajax.send();
					return data;
				} catch (error) {
					if (window.Alerta && Alerta.TratamentoErroComLinha) {
						Alerta.TratamentoErroComLinha("Pages/Shared/_ScriptsBasePlugins.cshtml", "loadJsonSync", error);
					} else {
						console.error('Erro em loadJsonSync:', error);
					}
					return null;
				}
			}

			var cldr = [
				loadJsonSync('https://cdn.syncfusion.com/ej2/32.1.19/dist/cldr-data/supplemental/numberingSystems.json'),
				loadJsonSync('https://cdn.syncfusion.com/ej2/32.1.19/dist/cldr-data/supplemental/weekData.json'),
				loadJsonSync('https://cdn.syncfusion.com/ej2/32.1.19/dist/cldr-data/main/pt-BR/numbers.json'),
				loadJsonSync('https://cdn.syncfusion.com/ej2/32.1.19/dist/cldr-data/main/pt-BR/ca-gregorian.json'),
				loadJsonSync('https://cdn.syncfusion.com/ej2/32.1.19/dist/cldr-data/main/pt-BR/timeZoneNames.json'),
				loadJsonSync('https://cdn.syncfusion.com/ej2/32.1.19/dist/cldr-data/main/pt-BR/currencies.json')
			].filter(Boolean);

			if (cldr.length) {
				ej.base.loadCldr.apply(ej.base, cldr);
			}

			var locale = loadJsonSync('https://cdn.syncfusion.com/ej2/32.1.19/dist/ej2-locale/pt-BR.json');
			if (locale) {
				ej.base.L10n.load(locale);
			}

			ej.base.setCulture('pt-BR');
			window.__cldrLoaded = true;
		} catch (error) {
			if (window.Alerta && Alerta.TratamentoErroComLinha) {
				Alerta.TratamentoErroComLinha("Pages/Shared/_ScriptsBasePlugins.cshtml", "SyncfusionCLDR", error);
			} else {
				console.error('Erro ao carregar CLDR pt-BR:', error);
			}
			window.__cldrLoaded = false;
		}
	})();
</script>
